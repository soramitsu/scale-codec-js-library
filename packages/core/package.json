{
  "name": "@scale-codec/core",
  "version": "2.0.1",
  "description": "Low-level tools to perform serialization and deserialization via SCALE spec",
  "license": "Apache-2.0",
  "keywords": [
    "SCALE",
    "codec",
    "binary",
    "parity-scale-codec",
    "serialization",
    "typescript",
    "rust",
    "encoding"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soramitsu/scale-codec-js-library.git"
  },
  "bugs": {
    "url": "https://github.com/soramitsu/scale-codec-js-library/issues"
  },
  "homepage": "https://soramitsu.github.io/scale-codec-js-library/",
  "engines": {
    "node": ">=14.5.0"
  },
  "engineStrict": true,
  "main": "./dist/lib.cjs",
  "module": "./dist/lib.mjs",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.mjs",
      "require": "./dist/lib.cjs"
    },
    "./*": "./*"
  },
  "typedoc": {
    "entryPoint": "./src/lib.ts",
    "tsconfig": "../../tsconfig.typedoc.json"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@scale-codec/enum": "workspace:^2.1.1",
    "@scale-codec/util": "workspace:^1.1.2"
  },
  "devDependencies": {
    "hada": "^0.0.8",
    "type-fest": "^3.5.5"
  }
}
