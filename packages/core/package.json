{
    "name": "@scale-codec/core",
    "version": "0.4.2",
    "description": "Low-level tools to perform serialization and deserialization via SCALE spec",
    "license": "Apache-2.0",
    "keywords": [
        "SCALE",
        "codec",
        "binary",
        "parity-scale-codec",
        "serialization",
        "typescript",
        "rust",
        "encoding"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/soramitsu/scale-codec-js-library.git"
    },
    "bugs": {
        "url": "https://github.com/soramitsu/scale-codec-js-library/issues"
    },
    "homepage": "https://soramitsu.github.io/scale-codec-js-library/",
    "main": "src/lib.ts",
    "engines": {
        "node": ">=14.5.0"
    },
    "engineStrict": true,
    "publishConfig": {
        "main": "./dist/lib.cjs.js",
        "module": "./dist/lib.esm.js",
        "types": "./dist/lib.d.ts",
        "exports": {
            ".": {
                "import": "./dist/lib.esm.js",
                "require": "./dist/lib.cjs.js"
            },
            "./*": "./*"
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@scale-codec/enum": "workspace:^0.2.2",
        "@scale-codec/util": "workspace:^0.1.3"
    },
    "devDependencies": {
        "benny": "^3.7.1",
        "esno": "^0.10.1",
        "hada": "^0.0.8",
        "type-fest": "^2.5.1",
        "scale-core-npm": "npm:@scale-codec/core@0.4.1"
    },
    "scripts": {
        "benchmark": "esno benchmark/run"
    }
}
