{
    "name": "@scale-codec/namespace-next",
    "version": "0.1.0",
    "main": "dist/lib.cjs.js",
    "module": "dist/lib.esm.js",
    "types": "dist/lib.d.ts",
    "exports": {
        ".": {
            "require": "./dist/lib.cjs.js",
            "import": "./dist/lib.esm.js"
        },
        "./std/str": {
            "require": "./dist/std/str/index.cjs.js",
            "import": "./dist/std/str/index.esm.js"
        }
    },
    "scripts": {
        "example": "esno example",
        "roll": "rm -rf dist && rollup -c rollup.config.js",
        "build:declaration": "tsc -p tsconfig.declaration.json",
        "gen-std-integers": "esno scripts/gen-std-integers"
    },
    "dependencies": {
        "@scale-codec/core": "workspace:^0.1.0",
        "@scale-codec/enum": "workspace:^0.1.0",
        "@scale-codec/util": "workspace:^0.1.0",
        "jsbi": "3.1.5",
        "map-obj": "^4.2.1"
    },
    "devDependencies": {
        "esbuild-register": "^2.6.0",
        "esno": "^0.8.0",
        "rollup": "^2.56.2",
        "rollup-plugin-dts": "^3.0.2",
        "rollup-plugin-esbuild": "^4.5.0",
        "typescript": "^4.3.5"
    }
}
