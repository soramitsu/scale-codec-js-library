{
    "name": "@scale-codec/definition-runtime",
    "version": "0.1.3",
    "description": "Runtime for compiled result from @scale-codec/definition-compiler",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/soramitsu/scale-codec-js-library.git"
    },
    "bugs": {
        "url": "https://github.com/soramitsu/scale-codec-js-library/issues"
    },
    "homepage": "https://github.com/soramitsu/scale-codec-js-library/tree/master/packages/definition-runtime#readme",
    "main": "src/lib.ts",
    "publishConfig": {
        "main": "./dist/lib.cjs.js",
        "module": "./dist/lib.esm.js",
        "types": "./dist/lib.d.ts",
        "exports": {
            ".": {
                "import": "./dist/lib.esm.js",
                "require": "./dist/lib.cjs.js"
            },
            "./*": "./*"
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@scale-codec/core": "workspace:^0.2.3",
        "@scale-codec/util": "workspace:^0.1.1"
    }
}
