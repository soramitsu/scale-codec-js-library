{
    "name": "@scale-codec/definition-compiler",
    "version": "0.2.0",
    "description": "SCALE-codec types namespace definition compiler",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/soramitsu/scale-codec-js-library.git"
    },
    "bugs": {
        "url": "https://github.com/soramitsu/scale-codec-js-library/issues"
    },
    "homepage": "https://github.com/soramitsu/scale-codec-js-library/tree/master/packages/definition-compiler#readme",
    "main": "src/lib.ts",
    "publishConfig": {
        "main": "./dist/lib.cjs.js",
        "module": "./dist/lib.esm.js",
        "types": "./dist/lib.d.ts",
        "exports": {
            ".": {
                "import": "./dist/lib.esm.js",
                "require": "./dist/lib.cjs.js"
            },
            "./*": "./*"
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "prettier": "^2.4.1",
        "sort-es": "^1.3.10",
        "vue": "^3.2.6"
    }
}
